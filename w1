public class shuzhu2 {
    public static void main(String[] args) {
        int []a={111,45,46,67,76,666,78,8,-800,678};
        System.out.println(min(a));
    }
    public static int min(int a[]){
        int min = a[0];
        for (int i = 1; i < a.length; i++){
            if (a[i]<min){
                min = a[i];
            }
        }
        return min;
    }
}
